{
  "name": "sli-web-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "yarn localstack:rm && yarn build && yarn localstack:cp",
    "localstack:ls": "aws --endpoint-url=http://localhost:4566 s3 ls",
    "localstack:mb": "aws --endpoint-url=http://localhost:4566 s3 mb s3://sli-web-app",
    "localstack:rm": "aws --endpoint-url=http://localhost:4566 s3 rm s3://sli-web-app --recursive",
    "localstack:cp": "aws --endpoint-url=http://localhost:4566 s3 cp build s3://sli-web-app --exclude \"_next/cache/*\" --recursive",
    "prepare": "husky install",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.16.0",
    "@azure/msal-react": "^1.0.1",
    "@chakra-ui/icons": "^1.0.15",
    "@chakra-ui/react": "^1.6.5",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "axios": "^0.21.1",
    "framer-motion": "^4.1.17",
    "next": "11.0.1",
    "nprogress": "^0.2.0",
    "qrcode": "^1.4.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.12.2",
    "react-icons": "^4.2.0",
    "swr": "^1.0.0",
    "zustand": "^3.5.10"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.5.0",
    "@testing-library/react": "^12.0.0",
    "@types/nprogress": "^0.2.0",
    "@types/qrcode": "^1.4.1",
    "@types/react": "17.0.15",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.25.2",
    "husky": "^7.0.0",
    "prettier": "2.4.1",
    "typescript": "4.3.5"
  }
}
